================ Positive examples ================
Bart giggles
(S[]
  (NP[NUM='sg', PER=3] (ProperNoun[NUM='sg', PER=3] Bart))
  (VP[NUM='sg', PER=?p, TENSE=?t]
    (V[FORM='pres', NUM='sg', SUBCAT='intrans'] giggles)))
Homer giggled
(S[]
  (NP[NUM='sg', PER=3] (ProperNoun[NUM='sg', PER=3] Homer))
  (VP[NUM=?n, PER=?p, TENSE='pastpart']
    (V[SUBCAT='intrans', TENSE='pastpart'] giggled)))
(S[]
  (NP[NUM='sg', PER=3] (ProperNoun[NUM='sg', PER=3] Homer))
  (VP[NUM=?n, PER=?p, TENSE='past']
    (V[SUBCAT='intrans', TENSE='past'] giggled)))
Bart and Lisa drank milk
(S[]
  (NP[NUM='sg', PER=3]
    (ProperNoun[NUM='sg', PER=3] Bart)
    (CONJ[] and)
    (ProperNoun[NUM='sg', PER=3] Lisa))
  (VP[NUM=?n, PER=?p, TENSE=?t]
    (TV[NUM=?n, PER=?p, SUBCAT='trans', TENSE=?t]
      (V[NUM=?n, PER=?p, SUBCAT='trans', TENSE='past'] drank))
    (NP[NUM=?n, PER=?p] (Noun[NUM='sg'] milk))))
Bart wears blue shoes
(S[]
  (NP[NUM='sg', PER=3] (ProperNoun[NUM='sg', PER=3] Bart))
  (VP[NUM=?n, PER=?p, TENSE=?t]
    (TV[NUM=?n, PER=?p, SUBCAT='trans', TENSE=?t]
      (V[NUM='sg', PER=3, SUBCAT='trans', TENSE='pres'] wears))
    (NP[NUM=?n, PER=?p]
      (ADJP[NUM=?n, PER=?p, SUBCAT=?rest, TENSE=?t] (ADJ[] blue))
      (NP[NUM=?n, PER=?p] (Noun[NUM='pl'] shoes)))))
Homer serves Lisa and Bart a healthy green salad
(S[]
  (NP[NUM='sg', PER=3] (ProperNoun[NUM='sg', PER=3] Homer))
  (VP[NUM='sg', PER=3, TENSE=?t]
    (TV[NUM=?n, PER=?p, SUBCAT='trans', TENSE=?t]
      (V[NUM='sg', PER=3, SUBCAT='trans', TENSE='pres'] serves))
    (NP[NUM='sg', PER=3]
      (ProperNoun[NUM='sg', PER=3] Lisa)
      (CONJ[] and)
      (ProperNoun[NUM='sg', PER=3] Bart))
    (ARG[CAT='np', NUM='sg', PER=?p]
      (NP[NUM='sg', PER=?p]
        (ADJP[NUM='sg', PER=?p, SUBCAT=?rest, TENSE=?t]
          (DET[NUM='sg'] a)
          (ADJ[] healthy)
          (ARG[CAT='adjunct', HEAD=[NUM=?n, PER=?p, TENSE=?t]]
            (ADJP[NUM=?n, PER=?p, SUBCAT=?rest, TENSE=?t]
              (ADJ[] green))))
        (Noun[NUM='sg'] salad)))))
