Homer and Bart never drink milk in the kitchen after lunch
(S[]
  (NP[NUM='sg', PER=3]
    (ProperNoun[NUM='sg', PER=3] Homer)
    (CONJ[] and)
    (ProperNoun[NUM='sg', PER=3] Bart))
  (VP[NUM=?n, PER=?p, TENSE=?t]
    (ADV[] never)
    (VP[NUM=?n, PER=?p, TENSE=?t]
      (TV[NUM=?n, PER=?p, SUBCAT='trans', TENSE=?t]
        (V[NUM='pl', SUBCAT='trans', TENSE='pres'] drink))
      (NP[NUM=?n, PER=?p]
        (NP[NUM=?n, PER=?p]
          (NP[NUM=?n, PER=?p] (Noun[NUM='sg'] milk))
          (PP[NUM=?n, PER=?p, TENSE=?t]
            (PREP[] in)
            (DET[NUM='pl'] the)
            (NP[NUM=?n, PER=?p] (Noun[NUM='sg'] kitchen))))
        (PP[NUM=?n, PER=?p, TENSE=?t]
          (PREP[] after)
          (NP[NUM=?n, PER=?p] (Noun[NUM='sg'] lunch)))))))
